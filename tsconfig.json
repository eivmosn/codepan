{
  "compilerOptions": {
    "target": "ES2015",
    "jsx": "preserve",
    "jsxImportSource": "vue",
    "lib": ["esnext", "dom"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "types": [
      "vite/client"
    ],
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  },
  "exclude": ["node_modules"]
}
